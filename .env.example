# Twitter Bot Configuration

# Target profile URL (can use twitter.com or x.com domain)
TARGET_PROFILE=https://x.com/u235___

# Server port for the API
PORT=3000

# Refresh interval in seconds (random between MIN and MAX)
MIN_REFRESH_TIME=15
MAX_REFRESH_TIME=20

# Delay between launching multiple bots (in seconds)
BOT_LAUNCH_DELAY=10

# Set to "true" to run bots in headless mode (no visible browser)
HEADLESS=true 